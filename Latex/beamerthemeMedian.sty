\newcommand{\bigtemplate}[2]{{
		\setbeamertemplate{background}{
			\begin{tikzpicture}
				\useasboundingbox (0,0) rectangle(\the\paperwidth,\the\paperheight);
				\draw (\the\paperwidth*0.55,\the\paperheight*0.79) node[below, fill=EDD_gray,thick,text width=11.5cm,minimum height=\the\paperheight*0.1] (tit){
					\begin{beamercolorbox}{section page}
						\LARGE        #1
					\end{beamercolorbox}

				};
				\node[align=center,below = of tit]  {
					\begin{beamercolorbox}{}
						{\usebeamerfont{date}
							\begin{center}
								\noexpand{#2}
							\end{center}
						}
				\end{beamercolorbox}};
				\node[rectangle,fill = EDD_green,minimum width = \the\paperwidth*0.15,minimum height=\the\paperheight*0.1,below] (r) at (0,\the\paperheight*0.79) {\vphantom{\parbox{9cm}{ \LARGE        #1}}};

			\end{tikzpicture}
		}
		\setbeamertemplate{logo}{
			\begin{tikzpicture}
				\useasboundingbox (0,0) rectangle(\the\paperwidth,\the\paperheight);
				\node[align=center] (tit)  at (\the\paperwidth*0.52,\the\paperheight*0.9)
				{\IfFileExists{imagenes/Logo1.png}{\includegraphics[height=1.50cm]{imagenes/Logo1.png}}{}
					\IfFileExists{imagenes/Logo2.png}{\hspace{1cm}\includegraphics[height=1.50cm]{imagenes/Logo2.png}}{}
				\IfFileExists{imagenes/Logo3.png}{\hspace{1cm}\includegraphics[height=1.50cm]{imagenes/Logo3.png}}{}};
			\end{tikzpicture}
		}
		\frame{}
}}
