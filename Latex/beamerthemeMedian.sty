\newcommand{\MyIncludePic}[4]{
	\IfFileExists{#1}{
		\begin{tikzpicture}
			\node[align=center] (tit)  at (current bounding box.center){
			\includegraphics[width=#2]{#1}};

			\IfEmptyTF{#4}%
			{}%
			{\node[align=right,text width=#2,above]   at (tit.north){
			\tiny \color{MetroLightBlue}#4};}
			\IfEmptyTF{#3}%
			{}%
			{\node[align=justify,text width=#2,below]   at (tit.south){
			\tiny \begin{spacing}{0.5}  \fontdimen2\font=0.5ex #3\end{spacing}};}
		\end{tikzpicture}
	}{ #1 does not exist}
}

