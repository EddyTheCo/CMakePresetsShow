		\tikzstyle{mybox} = [draw=EDD_gray,  very thick,
		rectangle, rounded corners, inner sep=4pt]
		\begin{tikzpicture}
			\node [mybox,align=center,text width=#4] (box)
			{
				\begin{minipage}{#4}
					\begin{center}
						\noexpand{\setbeamertemplate{items}[square] #2}
					\end{center}
				\end{minipage}
			};
			\IfEmptyTF{#1}%
			{}%
			{
				\node[uptitle,right=0.2,align=left] at (box.north west) {
					%				   \begin{minipage}{box.width}
					\small #1
					%				   \end{minipage}
			};}
			\IfEmptyTF{#3}%
			{}%
			{\node[uptitle,left=0.2,align=right] at (box.south east) {
					\tiny			   #3
			};}

		\end{tikzpicture}
}}
